{
  "name": "@poviolabs/spa-deploy-cli",
  "version": "2.1.0",
  "packageManager": "yarn@3.2.1",
  "bin": "./dist/sh.js",
  "scripts": {
    "build": "yarn clean && yarn version patch && yarn run build:cli",
    "build:cli": "tsc -p ./tsconfig.json && chmod +x ./dist/sh.js",
    "clean": "node tools/cleanup",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "test:cli": "./dist/sh.js --pwd ./test --stage test",
    "test:ts-node:cli": "ts-node -r tsconfig-paths/register ./src/sh.ts --pwd ./test --stage test",
    "lint": "eslint --fix"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "povio",
    "spa",
    "deploy",
    "aws",
    "s3",
    "cloudfront"
  ],
  "homepage": "https://github.com/poviolabs/spa-deploy-cli",
  "bugs": "https://github.com/poviolabs/spa-deploy-cli/issues",
  "repository": {
    "url": "https://github.com/poviolabs/spa-deploy-cli",
    "type": "git"
  },
  "contributors": [
    "Marko Zabreznik <marko.zabreznik@poviolabs.com>"
  ],
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.92.0",
    "@aws-sdk/client-s3": "^3.88.0",
    "@aws-sdk/client-sts": "^3.87.0",
    "@aws-sdk/credential-provider-env": "^3.110.0",
    "@aws-sdk/credential-provider-ini": "^3.112.0",
    "@aws-sdk/node-config-provider": "^3.110.0",
    "node-stage": "poviolabs/node-stage#v1",
    "yargs": "^17.5.1",
    "fast-glob": "^3.2.11",
    "micromatch": "^4.0.5",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.110.0",
    "@jest/types": "^28.1.1",
    "@swc/core": "^1.2.204",
    "@swc/jest": "^0.2.21",
    "@types/diff": "^5.0.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^16.11.41",
    "@types/prettier": "^2.6.3",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">= 16.15.1",
    "yarn": ">= 3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    }
  }
}
